<UserControl x:Class="PPTVForWP7.Controls.MarkImageItem"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    FontFamily="{StaticResource PhoneFontFamilyNormal}"
    FontSize="{StaticResource PhoneFontSizeNormal}"
    Foreground="{StaticResource PhoneForegroundBrush}"
    d:DesignHeight="480" d:DesignWidth="480" Width="115" Height="29">

    <Grid x:Name="LayoutRoot">
        <StackPanel Orientation="Horizontal">
            <Image x:Name="One" Width="23" Height="30" HorizontalAlignment="Left" VerticalAlignment="Center"/>
            <Image x:Name="Two" Width="23" Height="30" HorizontalAlignment="Left" VerticalAlignment="Center"/>
            <Image x:Name="Three" Width="23" Height="30" HorizontalAlignment="Left" VerticalAlignment="Center"/>
            <Image x:Name="Four" Width="23" Height="30" HorizontalAlignment="Left" VerticalAlignment="Center"/>
            <Image x:Name="Five" Width="23" Height="30" HorizontalAlignment="Left" VerticalAlignment="Center"/>
        </StackPanel>
    </Grid>
</UserControl>
